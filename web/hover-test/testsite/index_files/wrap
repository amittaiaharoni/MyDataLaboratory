/*Sirdata Amelie / Fhardi 191015*/
(function(){
var service = 'product.d';
var config = {

pa : "21137", // Sirdata Advertiser ID
u : "", // URL but leave empty if necessary
r : "", // REFERRER but leave empty if necessary
product_id : TDConf.Config.productId, // Collect product id
product_name : TDConf.Config.productName, // COllect product name
product_price : TDConf.Config.price // Price

};
var configParams = [];
for(var v in config){
configParams .push(v+'='+encodeURIComponent(config[v]));
}
  var sddanJS=document.createElement('script');
  sddanJS.async = true;
  sddanJS.src='//js.sddan.com/'+service+'?'+ configParams.join('&');
 document.getElementsByTagName('head')[0].appendChild(sddanJS);
 })();