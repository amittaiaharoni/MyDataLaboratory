<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://symfony.com/schema/routing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">
  <route id="my_data_lab_white_paper_homepage" path="/">
    <default key="_controller">MyDataLabWhitePaperBundle:WhitePaper:index</default>
  </route>
  <route id="my_data_lab_white_paper_new" path="/new">
    <default key="_controller">MyDataLabWhitePaperBundle:WhitePaper:new</default>
  </route>
  <route id="my_data_lab_white_paper_create" path="/create" methods="POST">
    <default key="_controller">MyDataLabWhitePaperBundle:WhitePaper:create</default>
  </route>
  <route id="my_data_lab_white_paper_update" path="/{id}">
    <default key="_controller">MyDataLabWhitePaperBundle:WhitePaper:update</default>
  </route>
  <route id="my_data_lab_white_paper_delete" path="/delete/{id}" methods="DELETE">
    <default key="_controller">MyDataLabWhitePaperBundle:WhitePaper:delete</default>
  </route>
</routes>
