<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="MyDataLab\Bundle\ProductBundle\Entity\Subscriber" table="subscribers">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="firstName" type="string" column="first_name" length="50" nullable="false">
            <options>
                <option name="fixed"/>
                <option name="default">0</option>
            </options>
        </field>
        <field name="lastName" type="string" column="last_name" length="50" nullable="false">
            <options>
                <option name="fixed"/>
                <option name="default">0</option>
            </options>
        </field>
        <field name="email" type="string" column="email" length="50" nullable="false">
            <options>
                <option name="fixed"/>
                <option name="default">0</option>
            </options>
        </field>
        <field name="companyName" type="string" column="company_name" length="50" nullable="false">
            <options>
                <option name="fixed"/>
                <option name="default">0</option>
            </options>
        </field>
        <field name="website" type="string" column="website" length="100" nullable="false">
            <options>
                <option name="fixed"/>
                <option name="default">0</option>
            </options>
        </field>
        <field name="telephone" type="string" column="telephone" length="40" nullable="false">
            <options>
                <option name="fixed"/>
                <option name="default">0</option>
            </options>
        </field>
        <field name="date" type="datetime" column="date" nullable="false">
        </field>
    </entity>
</doctrine-mapping>
