<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="my_data_lab_wording_homepage" path="/">
        <default key="_controller">MyDataLabDashboardBundle:Wording:index</default>
    </route>
    <route id="my_data_lab_wording_dashboard" path="/dashboard">
        <default key="_controller">MyDataLabDashboardBundle:Wording:dashboard</default>
    </route>
    <route id="my_data_lab_wording_keywords_list" path="/list-keywords">
        <default key="_controller">MyDataLabDashboardBundle:Wording:keywordsList</default>
    </route>
    <route id="my_data_lab_wording_product_analysis" path="/product-analysis">
        <default key="_controller">MyDataLabDashboardBundle:Wording:productAnalysis</default>
    </route>
</routes>
