<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity repository-class="MyDataLab\Bundle\CoreBundle\Repository\LanguageRepository" name="MyDataLab\Bundle\CoreBundle\Entity\Language" table="mdl_languages">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="code" type="string" column="code" length="2" unique="true"/>
        <field name="name" type="string" column="name" length="255" unique="true"/>
    </entity>
</doctrine-mapping>
