<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="my_data_lab_user_homepage" path="/">
        <default key="_controller">MyDataLabUserBundle:Default:index</default>
    </route>
    <route id="my_data_lab_user_account" path="/account">
        <default key="_controller">MyDataLabUserBundle:Default:account</default>
    </route>
    <route id="my_data_lab_user_invoice" path="/invoice">
        <default key="_controller">MyDataLabUserBundle:Default:invoice</default>
    </route>
</routes>
