{% extends 'MyDataLabAdminBundle::base.html.twig' %}
{% block title %}Admin Clients Dashboard{% endblock %}
{% block content %}
    <div class="row">
        <section id="sortable1" class="connectedSortable col-lg-3">
            <div class="small-box products">
                <div class="inner">
                    <h3>45</h3>
                    <p>Active Clients</p>
                    <div class="procent3">
                        <p><i class="ion ion-arrow-up-a"></i></p>
                        <p>+ 10%</p>
                    </div>
                </div>
                <div class="icon"> <i class="ion ion-person-add"></i> </div> <a href="#" class="small-box-footer">{% filter capitalize %}{% trans %}MORE_INFO{% endtrans %}{% endfilter %} <i class="fa fa-arrow-circle-right"></i></a> </div>
        </section>
        <section id="sortable2" class="connectedSortable col-lg-3">
            <div class="small-box e-retailers">
                <div class="inner">
                    <h3>25</h3>
                    <p>E-retailers</p>
                    <div class="procent3">
                        <p><i class="ion ion-arrow-up-a"></i> </p>
                        <p>+ 10%</p>
                    </div>
                </div>
                <div class="icon"> <i class="fa fa-shopping-cart"></i> </div> <a href="#" class="small-box-footer">{% filter capitalize %}{% trans %}MORE_INFO{% endtrans %}{% endfilter %} <i class="fa fa-arrow-circle-right"></i></a> </div>
        </section>
        <section id="sortable3" class="connectedSortable col-lg-3">
            <div class="small-box brands">
