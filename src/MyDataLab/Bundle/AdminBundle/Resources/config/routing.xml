    </route>
    <route id="my_data_lab_admin_dashboard" path="/">
        <default key="_controller">MyDataLabAdminBundle:Admin:dashboard</default>
    </route>
    <import resource="@MyDataLabAdminBundle/Resources/config/routing/clients.xml" prefix="/clients/"/>
    <import resource="@MyDataLabAdminBundle/Resources/config/routing/users.xml" prefix="/users/"/>
    <import resource="@MyDataLabAdminBundle/Resources/config/routing/turnovers.xml" prefix="/turnover/"/>
    <import resource="@MyDataLabAdminBundle/Resources/config/routing/database.xml" prefix="/database/"/>
    <import resource="@MyDataLabBlogBundle/Resources/config/routing.xml" prefix="/blog/"/>
    <import resource="@MyDataLabGlossaryBundle/Resources/config/routing.xml" prefix="/glossary/"/>
    <import resource="@MyDataLabWhitePaperBundle/Resources/config/routing.xml" prefix="/white-papers/"/>
    <import resource="@MyDataLabTranslationBundle/Resources/config/routing.xml" prefix="/translation/"/>
    <import resource="@MyDataLabUserBundle/Resources/config/routing.xml" prefix="/account/" />
    <import resource="@MyDataLabWordingBundle/Resources/config/routing.xml" prefix="/wording/" />
    <import resource="@MyDataLabCatalogsBundle/Resources/config/routing.xml" prefix="/catalogue/" />
    <import resource="@MyDataLabAlertsBundle/Resources/config/routing.xml" prefix="/alerts/" />
    <import resource="@MyDataLabPricesBundle/Resources/config/routing.xml" prefix="/prices/" />
