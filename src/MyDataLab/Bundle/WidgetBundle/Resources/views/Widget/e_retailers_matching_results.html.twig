<div class="box mdl-widget-doughnut mdl-widget" data-widget-id="{{ widget.id }}">
    <div class="box-header with-border">
        <button type="button" class="btn btn-primary btn-sm pull-right" data-widget="collapse" data-toggle="tooltip" title="Collapse" style="margin-right: 5px;">
            <i class="fa fa-minus"></i>
        </button>
        <h3 class="box-title">{% filter title %}{% trans %}MATCHING_RESULTS{% endtrans %}{% endfilter %}</h3>
    </div>
    <table class="mdl-widget-source-table" style="display: none;">
        <thead>
            <tr>
                <th>Status</th>
                <th>Nr. of products</th>
            </tr>
        </thead>
        <tbody>
            <tr style="background-color: #99CA3C">
                <td>Matched products</td>
                <td>20</td>
            </tr>
            <tr style="background-color: #E12010">
                <td>Not Matched products</td>
                <td>11</td>
            </tr>
            <tr style="background-color: #D7760F">
                <td>Waiting for match</td>
                <td>4</td>
            </tr>
        </tbody>
    </table>
    <div class="box-body chart" id="chart_div2" style="min-height:230px"> </div>
    <div class="box-footer"></div>
</div>