<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="my_data_lab_widget_save_order" path="/save-widgets-order" methods="POST">
        <default key="_controller">MyDataLabWidgetBundle:Widget:saveOrder</default>
    </route>
    <route id="my_data_lab_widget_reset" path="/reset-widgets-order" methods="POST">
        <default key="_controller">MyDataLabWidgetBundle:Widget:resetOrder</default>
    </route>
</routes>
